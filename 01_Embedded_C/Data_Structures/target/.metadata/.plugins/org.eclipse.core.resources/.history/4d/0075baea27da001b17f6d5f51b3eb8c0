#include <stdio.h>
#include <stdint.h>
#include <stdlib.h>


struct node{
	int data;
	struct node *next;
};

typedef struct node nodeType;

nodeType * linkedlist_create(int32_t * arr, uint32_t arr_len);

int main(){

	int32_t task_pri[] = {0,2,4,5,1,4,6};

	nodeType *pri_list = linkedlist_create(task_pri,7);


	printf("Priority list content is:");

	while(head != NULL){
		printf(" %d ", head->data);
		head = head->next;
	}
	printf("\n");


	for(;;);


}

nodeType * linkedlist_create(int32_t * arr, uint32_t arr_len){
	nodeType *head, *last, *temp;

	head = (nodeType *)malloc(sizeof(nodeType));

	if(!head){
			return head; //Low system memory
	 }

	head->data = arr[0];
	head->next = NULL;
	last = head;

	for(int i=1; i<arr_len;i++){
		temp = (nodeType *)malloc(sizeof(nodeType));

		if(!temp){
			return temp; //Low system memory
		}

		temp->data = arr[i];
		temp->next = NULL;
		last->next = temp;
		last = temp;
	}

	return head;

}














