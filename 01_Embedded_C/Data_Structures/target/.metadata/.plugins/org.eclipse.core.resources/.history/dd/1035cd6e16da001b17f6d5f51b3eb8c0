	uint8_t x;
	uint8_t y;
};

union COORD2{
	uint8_t x;
	uint8_t y;
};

typedef struct COORD1 coord1Type;
typedef union COORD2 coord2Type;


int main(){

	coord1Type location1 = {3, 4};
	coord1Type location2 = {2}; //In union, you can assign value to just a single at a time

