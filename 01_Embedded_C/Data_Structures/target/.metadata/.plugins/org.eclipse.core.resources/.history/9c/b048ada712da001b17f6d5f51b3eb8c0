#include <stdio.h>
#include <stdint.h>
#include <stdlib.h>


struct shape{
	uint8_t type;
	int32_t x_coord;
	int32_t y_coord;
};

typedef struct shape shapeType;
/*
 1= rectangle
 2= square
 3= circle
 4= triangle
 */

int main(){

	shapeType button ={1, 5, -2};

	button.type = 2;
	button.x_coord = -10;
	button.y_coord = -30;

	shapeType panel;
	shapeType *pnl;

	pnl = &panel;
	pnl->type = 2;
	pnl->x_coord = 500;
	pnl->y_coord = -500;

	printf("The panel type is %d\r\n", pnl->type);
	printf("The panel x_coordinate is  %d \r\n", pnl->x_coord);
	printf("The panel y_coordinate is  %d  \r\n", pnl->y_coord);

	for(;;);




}
