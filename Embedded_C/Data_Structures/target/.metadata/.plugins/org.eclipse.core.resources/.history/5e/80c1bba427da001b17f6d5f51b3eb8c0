#include <stdio.h>
#include <stdint.h>
#include <stdlib.h>


struct node{
	int data;
	struct node *next;
};

typedef struct node nodeType;


int main(){

	int32_t val = 9;
	int32_t val2 = 29;

	nodeType *head, *last, *temp;

	head = (nodeType *)malloc(sizeof(nodeType));

	if(!head){
		return 0; //Low memory
 	}

	//Store data
	head->data = val;
	head->next = NULL;
	last = head;

	//Add a new node at the beginning of the list

	temp = (nodeType *)malloc(sizeof(nodeType));

	if(!temp){
		return 0; //Low memory
	}

	temp->data = val2;
	temp->next = head;
	head = temp;

	printf("Linked list content is:");

	while(head != NULL){
		printf(" %d ", head->data);
		head = head->next;
	}
	printf("\n");


	for(;;);


}

nodeType * linkedlist_create(int32_t * arr, uint32_t arr_len){
	nodeType *head, *last, *temp;

	head = (nodeType *)malloc(sizeof(nodeType));

	if(!head){
			return head; //Low system memory
	 }

	head->data = arr[0];
	head->next = NULL;
	last = head;

	for(int i=1; i<arr_len;i++){
		temp = (nodeType *)malloc(sizeof(nodeType));

		if(!temp){
			return temp; //Low system memory
		}

		temp->data = arr[i];
		temp->next = NULL;
		last->next = temp;
		last = temp;
	}

	return head;

}














